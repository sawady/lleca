

fun printVector(v : Vec) {
    i := 0
    putChar(91)
    while i < #v {
        putNum(v[i])
        i := i + 1
        if i < #v {
            putChar(44)
            putChar(32)
        }
    }
    putChar(93)
    putChar(10)
}

fun mulVector(v : Vec, k : Int) {
    i := 0
    while i < #v {
        v[i] := v[i] * k
        i := i + 1
    }
}

fun main() {
    v := [1, 2, 3]
    printVector(v)
    mulVector(v, 2)
    printVector(v)
    mulVector(v, 3)
    printVector(v)

    v := [11, 22, 33, 44, 55, 66, 77, 88, 99]
    printVector(v)
    mulVector(v, 2)
    printVector(v)
    mulVector(v, 3)
    printVector(v)
}
