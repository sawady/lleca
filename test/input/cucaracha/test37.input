fun fila(x : Int, op : Int, y : Int) {

    if op == 1 {
        r := x == y
    }
    if op == 2 {
        r := x <= y
    }
    if op == 3 {
        r := x >= y
    }
    if op == 4 {
        r := x < y
    }
    if op == 5 {
        r := x > y
    }
    if op == 6 {
        r := x != y
    }

    pi(x) sep(op) pi(y) eq() pb(r) enter()
}

