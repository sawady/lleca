
fun par(x : Int) : Bool {
    if x == 0 {
        res := True
    } else {
        res := impar(x - 1)
    } 
    return res
}

fun impar(x : Int) : Bool {
    if x == 0 {
        res := False
    } else {
        res := par(x - 1)
    }
    return res
}

fun probar(x : Int, n : Int) {
    if x <= n {
        if par(x) {
           putChar(112)
        } else {
           putChar(105)
        }
        putNum(x)
        putChar(10)
        probar(x + 1, n)
    }
}

fun main() {
   probar(0, 100)
}
