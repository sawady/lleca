
fun suma_vectores(v : Vec, w : Vec) {
    i := 0
    while i < #v {
        v[i] := v[i] + w[i]
        i := i + 1
    }
}

fun printVector(v : Vec) {
    i := 0
    putChar(91)
    while i < #v {
        putNum(v[i])
        i := i + 1
        if i < #v {
            putChar(44)
            putChar(32)
        }
    }
    putChar(93)
    putChar(10)
}

fun main() {
    v := [1, 2, 3, 4, 5, 6, 7, 8, 9]
    w := [10, 20, 30, 40, 50, 60, 70, 80, 90]
    printVector(v)
    printVector(w)
    suma_vectores(v, w)
    printVector(v)
    printVector(w)
    suma_vectores(v, v)
    printVector(v)
    printVector(w)
    suma_vectores(w, w)
    printVector(v)
    printVector(w)
}
